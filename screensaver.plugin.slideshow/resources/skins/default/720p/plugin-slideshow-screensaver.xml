			</animation>
			<animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan2),8)" reversible="false">
				<effect type="slide" start="128,-72" end="-128,72" time="10500"/>
				<effect type="zoom" start="120" end="120" center="auto" time="10500"/>
			</animation>
			<animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan2),9)" reversible="false">
