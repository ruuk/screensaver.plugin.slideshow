<window type="window">
	<controls>
		<control type="image" id="1">
			<posx>0</posx>
			<posy>0</posy>
			<width>1280</width>
			<height>720</height>
			<aspectratio>keep</aspectratio>
			<colordiffuse>$INFO[Window.Property(SlideView.Dim)]</colordiffuse>
			<animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Slide1),0)" reversible="false">
				<effect type="slide" tween="cubic" easing="inout" start="1280,0" end="0,0" time="500"/>
			</animation>
			<animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Slide1),1)" reversible="false">
				<effect type="slide" tween="cubic" easing="inout" start="0,0" end="-1280,0" time="500"/>
			</animation>
			<animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Fade1),1)">
				<effect type="fade" start="100" end="0" time="500"/>
			</animation>
			<animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan1),1)" reversible="false">
				<effect type="zoom" start="100" end="120" center="auto" time="10500"/>
			</animation>
			<animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan1),2)" reversible="false">
				<effect type="slide" start="128,0" end="-128,0" time="10500"/>
				<effect type="zoom" start="120" end="120" center="auto" time="10500"/>
			</animation>
			<animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan1),3)" reversible="false">
				<effect type="slide" start="-128,0" end="128,0" time="10500"/>
				<effect type="zoom" start="120" end="120" center="auto" time="10500"/>
			</animation>
			<animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan1),4)" reversible="false">
				<effect type="slide" start="0,72" end="0,-72" time="10500"/>
				<effect type="zoom" start="120" end="120" center="auto" time="10500"/>
			</animation>
			<animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan1),5)" reversible="false">
				<effect type="slide" start="0,-72" end="0,72" time="10500"/>
				<effect type="zoom" start="120" end="120" center="auto" time="10500"/>
			</animation>
			<animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan1),6)" reversible="false">
				<effect type="slide" start="128,72" end="-128,-72" time="10500"/>
				<effect type="zoom" start="120" end="120" center="auto" time="10500"/>
			</animation>
			<animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan1),7)" reversible="false">
				<effect type="slide" start="-128,72" end="128,-72" time="10500"/>
				<effect type="zoom" start="120" end="120" center="auto" time="10500"/>
			</animation>
			<animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan1),8)" reversible="false">
				<effect type="slide" start="128,-72" end="-128,72" time="10500"/>
				<effect type="zoom" start="120" end="120" center="auto" time="10500"/>
			</animation>
			<animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan1),9)" reversible="false">
				<effect type="slide" start="-128,-72" end="128,72" time="10500"/>
				<effect type="zoom" start="120" end="120" center="auto" time="10500"/>
			</animation>
		</control>
		<control type="image" id="2">
			<posx>0</posx>
			<posy>0</posy>
			<width>1280</width>
			<height>720</height>
