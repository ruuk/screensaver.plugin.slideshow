				<effect type="fade" start="100" end="0" time="500"/>
			</animation>
			<animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan2),1)" reversible="false">
				<effect type="zoom" tween="linear" start="100" end="120" center="auto" time="10500"/>
			</animation>
			<animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan2),2)" reversible="false">
