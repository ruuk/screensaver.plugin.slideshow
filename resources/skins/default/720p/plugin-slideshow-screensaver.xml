<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<window type="window">
    <onload>SetProperty(loading_image,plugin-ss-screensaver-loading.gif)</onload>
    <controls>
        <control type="image" id="100">
            <posx>0</posx>
            <posy>0</posy>
            <width>1280</width>
            <height>720</height>
            <colordiffuse>FF000000</colordiffuse>
            <texture>plugin-ss-screensaver-white-box.png</texture>
        </control>
        <control type="image" id="1">
            <posx>0</posx>
            <posy>0</posy>
            <width>1280</width>
            <height>720</height>
            <aspectratio>keep</aspectratio>
            <colordiffuse>$INFO[Window.Property(SlideView.Dim)]</colordiffuse>
            <texture background="true"></texture>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Slide1),0)" reversible="false">
                <effect type="slide" tween="cubic" easing="inout" start="1280,0" end="0,0" time="500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Slide1),1)" reversible="false">
                <effect type="slide" tween="cubic" easing="inout" start="0,0" end="-1280,0" time="500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Fade1),1)">
                <effect type="fade" start="100" end="0" time="500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan1),1)" reversible="false">
                <effect type="zoom" start="100" end="120" center="auto" time="10500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan1),2)" reversible="false">
                <effect type="slide" start="128,0" end="-128,0" time="10500" />
                <effect type="zoom" start="120" end="120" center="auto" time="10500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan1),3)" reversible="false">
                <effect type="slide" start="-128,0" end="128,0" time="10500" />
                <effect type="zoom" start="120" end="120" center="auto" time="10500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan1),4)" reversible="false">
                <effect type="slide" start="0,72" end="0,-72" time="10500" />
                <effect type="zoom" start="120" end="120" center="auto" time="10500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan1),5)" reversible="false">
                <effect type="slide" start="0,-72" end="0,72" time="10500" />
                <effect type="zoom" start="120" end="120" center="auto" time="10500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan1),6)" reversible="false">
                <effect type="slide" start="128,72" end="-128,-72" time="10500" />
                <effect type="zoom" start="120" end="120" center="auto" time="10500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan1),7)" reversible="false">
                <effect type="slide" start="-128,72" end="128,-72" time="10500" />
                <effect type="zoom" start="120" end="120" center="auto" time="10500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan1),8)" reversible="false">
                <effect type="slide" start="128,-72" end="-128,72" time="10500" />
                <effect type="zoom" start="120" end="120" center="auto" time="10500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan1),9)" reversible="false">
                <effect type="slide" start="-128,-72" end="128,72" time="10500" />
                <effect type="zoom" start="120" end="120" center="auto" time="10500" />
            </animation>
        </control>
        <control type="image" id="2">
            <posx>0</posx>
            <posy>0</posy>
            <width>1280</width>
            <height>720</height>
            <aspectratio>keep</aspectratio>
            <colordiffuse>$INFO[Window.Property(SlideView.Dim)]</colordiffuse>
            <texture background="true"></texture>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Slide2),0)" reversible="false">
                <effect type="slide" tween="cubic" easing="inout" start="1280,0" end="0,0" time="500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Slide2),1)" reversible="false">
                <effect type="slide" tween="cubic" easing="inout" start="0,0" end="-1280,0" time="500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Fade1),0)">
                <effect type="fade" start="100" end="0" time="500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan2),1)" reversible="false">
                <effect type="zoom" tween="linear" start="100" end="120" center="auto" time="10500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan2),2)" reversible="false">
                <effect type="slide" start="128,0" end="-128,0" time="10500" />
                <effect type="zoom" start="120" end="120" center="auto" time="10500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan2),3)" reversible="false">
                <effect type="slide" start="-128,0" end="128,0" time="10500" />
                <effect type="zoom" start="120" end="120" center="auto" time="10500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan2),4)" reversible="false">
                <effect type="slide" start="0,72" end="0,-72" time="10500" />
                <effect type="zoom" start="120" end="120" center="auto" time="10500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan2),5)" reversible="false">
                <effect type="slide" start="0,-72" end="0,72" time="10500" />
                <effect type="zoom" start="120" end="120" center="auto" time="10500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan2),6)" reversible="false">
                <effect type="slide" start="128,72" end="-128,-72" time="10500" />
                <effect type="zoom" start="120" end="120" center="auto" time="10500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan2),7)" reversible="false">
                <effect type="slide" start="-128,72" end="128,-72" time="10500" />
                <effect type="zoom" start="120" end="120" center="auto" time="10500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan2),8)" reversible="false">
                <effect type="slide" start="128,-72" end="-128,72" time="10500" />
                <effect type="zoom" start="120" end="120" center="auto" time="10500" />
            </animation>
            <animation type="Conditional" condition="StringCompare(Window.Property(SlideView.Pan2),9)" reversible="false">
                <effect type="slide" start="-128,-72" end="128,72" time="10500" />
                <effect type="zoom" start="120" end="120" center="auto" time="10500" />
            </animation>
        </control>
        <control type="image" id="3">
            <posx>-10</posx>
            <posy>-10</posy>
            <width>10</width>
            <height>10</height>
            <texture background="true"></texture>
        </control>
        <control type="image" id="4">
            <posx>-10</posx>
            <posy>-10</posy>
            <width>10</width>
            <height>10</height>
            <texture background="true"></texture>
        </control>
        <control type="label" id="200">
            <posx>0</posx>
            <posy>640</posy>
            <width>1280</width>
            <height>64</height>
            <align>center</align>
            <color>FFFFFFFF</color>
            <shadowcolor>FF000000</shadowcolor>
            <font>font14</font>
        </control>
        <control type="group" id="10">
            <posx>608</posx>
            <posy>328</posy>
            <animation type="Conditional" reversible="false" condition="!StringCompare(Window.Property(stop_animation),1)" pulse="true">
                <effect type="rotatey" start="-60" end="60" time="10000" center="640,360" reversible="false" />
            </animation>
            <control type="image" id="100">
                <posx>0</posx>
                <posy>0</posy>
                <width>64</width>
                <height>64</height>
                <aspectratio>keep</aspectratio>
                <texture>$INFO[Window.Property(loading_image)]</texture>
            </control>
            <control type="label" id="101">
                <posx>-608</posx>
                <posy>112</posy>
                <width>1280</width>
                <height>64</height>
                <align>center</align>
                <label> Loading images... $INFO[Window.Property(pss_progress),,%]</label>
                <font>font14</font>
            </control>
        </control>
    </controls>
</window>
